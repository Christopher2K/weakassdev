{
  "name": "@weakassdev/shared",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "echo 'Do nothing'",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch --onSuccess \"yarn generate:type\"",
    "build": "tsup --clean --minify",
    "generate:type": "tsc --declaration --emitDeclarationOnly"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "require": "./dist/models/index.cjs",
      "import": "./dist/models/index.js"
    },
    "./validators": {
      "types": "./dist/validators.d.ts",
      "require": "./dist/validators.cjs",
      "import": "./dist/validators.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "models": [
        "dist/models/index.d.ts"
      ],
      "validators": [
        "dist/validators.d.ts"
      ],
      "utils": [
        "dist/utils.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@swc/core": "~1.3.74",
    "tsup": "~7.2.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "luxon": "~3.3.0",
    "zod": "3.20.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/models/index.ts",
      "src/validators.ts",
      "src/utils.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": false
  }
}
